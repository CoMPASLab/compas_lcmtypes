package senlcm;

// similar to bot_core_image_sync_t, this lcm type is meant to publish the host utime
// This utime is requested at "the same time" as the camera ticks.

struct timedrift_t
{
    int64_t sync_host;
    int64_t sync_ticks;
    double ticks_sec;
    int64_t host_before;
    int64_t host_after;
    int64_t ticks;
    int64_t host_utime;
}
