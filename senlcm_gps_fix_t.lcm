package senlcm;

/* Standard message for GPS fixes.
 */

struct gps_fix_t
{
    // Standard header.
    stdlcm.header_t header;

    // Latitude.
    // SI units = degrees.
    double latitude;

    // Longitude.
    // SI units = degrees.
    double longitude;

    // Altitude.
    // SI units = m.
    double altitude;
}
